if OneOS then
    OneOS.LoadAPI('/Programs/SlimeLib.program/slime')
    OneOS.ToolBarColour = colours.lime
    OneOS.ToolBarTextColour = colours.white
else
os.loadAPI("slime")
end
--os.loadAPI("Programs/SlimeLib.program/slime")
slime.advClear(colors.white,colors.white)
slime.titleBar("SlimeLib Launcher",colors.lime,colors.white,1)
slime.setColor(colors.white,colors.lime)
term.setCursorPos(3,4)
print("Coming Soon:")
print("   *Better Launcher")
print("   *Wiki, for documentation look at the code")
print("   *More Features")
term.setCursorPos(1,10)
print("  Press any key to exit...")
os.pullEvent("key")
slime.advClear()
